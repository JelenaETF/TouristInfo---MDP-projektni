<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane opacity="0.9" prefHeight="270.0" prefWidth="442.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.HotelsSceneController">
   <children>
      <TableView fx:id="hotelsTable" layoutX="20.0" layoutY="14.0" opacity="0.9" prefHeight="200.0" prefWidth="403.0">
        <columns>
          <TableColumn fx:id="name" prefWidth="87.0" text="Naziv" />
          <TableColumn fx:id="address" prefWidth="150.0" text="Adresa" />
            <TableColumn fx:id="telephone" prefWidth="81.0" text="Telefon" />
            <TableColumn fx:id="category" prefWidth="84.0" text="Kategorija" />
        </columns>
      </TableView>
      <Button layoutX="20.0" layoutY="234.0" mnemonicParsing="false" onAction="#openStageForAdding" opacity="0.9" style="-fx-control-inner-background: #fbf7ec;" text="Dodaj" />
      <Button layoutX="373.0" layoutY="234.0" mnemonicParsing="false" onAction="#delete" opacity="0.9" style="-fx-control-inner-background: #fbf7ec;" text="Ukloni" />
   </children>
</AnchorPane>
